{
  "name" : "Ex4",
  "version": "0.1",
  "manifest_version" : 2,
  "description" : "オプションで選んだ色の背景色に変える",
  "permissions" : [
    "tabs", "http://*/*"
  ],
  "options_page" : "options.html",
  "background" : {
    "scripts": ["background.js"]
  },
  "browser_action" : {
    "default_icon" : "icon.png",
    "default_title": "background"
  }
}